{
    "name": "asseco-voice/laravel-gelf",
    "description": "Laravel support for GELF logging",
    "keywords": [
        "php",
        "laravel",
        "microservices",
        "gelf",
        "logging",
        "graylog"
    ],
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^8.1",
        "asseco-voice/laravel-common": "^3.0",
        "asseco-voice/laravel-stomp": "^4.0",
        "graylog2/gelf-php": "^1.6",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.4.4",
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^8.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Asseco\\Gelf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asseco\\Gelf\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\Gelf\\GelfServiceProvider"
            ]
        }
    }
}
